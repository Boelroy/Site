---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>blog.boelroy.com</title>
  <link href="http://blog.boelroy.com/"/>
  <link type="application/atom+xml" rel="self" href="http://blog.boelroy.com"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.boelroy.com/</id>
  <author>
    <name>boelroy</name>
    <email>boelroy@live.com</email>
  </author>

  {% for post in site.posts limit: 10 %}
  <entry>
    <id>http://blog.boelroy.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://journal.stuffwithstuff.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>boelroy</name>
      <uri>http://blog.boelroy.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
